{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "object-curly-spacing": ["error", "always"]
  }
}
